<view class="main">
	<!-- header S -->
	<view class="shader ">
		<view class="havatar">
			<i-avatar
			 src="{{avatar}}"
			 size="large"
			 shape="circle"
			 i-class="havatar-avatar"
			/>
		</view>
		<view class="hnamer">
			<text class="text-df">{{name}}</text>
		</view>
	</view>
	<!-- 订单信息 S -->
	<view class="order">
		<van-cell
		 title="我的订单"
		 custom-class="order-cell"
		 is-link
		 url="/pages/orderList/index"
		 border="{{false}}"
		/>
		<van-tabbar
		 active="{{  }}"
		 custom-class="order-tabbar"
		 border="{{false}}"
		 active-color="#ed1c24"
		 bind:change="onChange"
		 fixed="{{false}}"
		>
			<van-tabbar-item icon="pending-payment" info="5">待付款</van-tabbar-item>
			<van-tabbar-item icon="paid" dot>待发货</van-tabbar-item>
			<van-tabbar-item icon="logistics">待收货</van-tabbar-item>
			<van-tabbar-item icon="comment-o">评价</van-tabbar-item>
			<van-tabbar-item icon="after-sale">售后</van-tabbar-item>
		</van-tabbar>
	</view>
