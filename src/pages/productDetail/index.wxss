Page {
  padding-bottom: 1rpx;
}

Page swiper {
  height: 582rpx;
}

Page swiper image {
  width: 100%;
  height: 582rpx;
}

Page .product {
  padding-top: 20rpx;
  margin-bottom: 20rpx;
  background: #ffffff;
  padding-bottom: 20px;
}

Page .product .matter {
  background: #ffffff;
}

Page .product .matter .min_price {
  font-size: 12px;
  color: #e4393c;
  font-weight: 700;
  line-height: 1.3;
  display: inline-block;
  margin-left: 40rpx;
}

Page .product .matter .min_price .price {
  color: #e4393c;
  font-weight: 700;
  line-height: 1.3;
  display: inline-block;
  font-size: 18px;
}

Page .product .matter .original_price {
  font-size: 12px;
  height: 20px;
  line-height: 20px;
  color: #888;
  margin-left: 10px;
  text-decoration: line-through;
  display: inline-block;
}

Page .product .matter .sales {
  display: inline-block;
  right: 10px;
  position: absolute;
  font-size: 12px;
  color: #888;
}

Page .product .title {
  padding: 3px 10px 0 !important;
  background-color: #fff;
  background: #ffffff;
  margin-top: 5px;
  width: 75%;
  display: inline-block;
  vertical-align: middle;
}

Page .product .share {
  display: inline-block;
  vertical-align: middle;
}

Page .product .share-button {
  height: 66rpx;
  line-height: 66rpx;
  size: 10;
  border-radius: 30rpx;
}

Page .product .title .title-name {
  font-size: 14px;
  font-weight: 600;
}

Page .product .title .title-description {
  font-size: 14px;
  color: #888;
}

Page .product .title .text {
  color: #051b28;
  line-height: 20px;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  max-height: 63px;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  overflow: hidden;
  word-break: break-all;
}

