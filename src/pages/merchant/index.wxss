Page .custom-card {
  width: 47%;
  display: inline-block;
  vertical-align: middle;
  margin-left: 10rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  background-color: #fff;
}

Page .custom-card-centre {
}

