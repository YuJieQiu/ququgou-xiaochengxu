}

Page .custom-card__thumb .custom-card__img {
  width: 100%;
  height: 385rpx;
}

Page .custom-card__content {
}

Page .custom-card__title {
  font-size: 14px;
  font-weight: 500;
  word-wrap: break-word;
  line-height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-bottom: 8rpm;
}

Page .custom-card__price {
}

Page .custom-card__price .now-price {
  display: inline-block;
  margin-left: 10rpx;
  color: #e93b3d;
  font-weight: 400;
  font-style: normal;
  font-size: 15px;
}

Page .custom-card__price .original-price {
  display: inline-block;
  margin-left: 15rpx;
  font-size: 12px;
  color: #888;
  text-decoration: line-through;
}
